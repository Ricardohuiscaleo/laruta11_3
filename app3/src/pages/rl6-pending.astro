    <script>
        let currentOrderData = null;

        function generateWhatsAppMessage() {
            const urlParams = new URLSearchParams(window.location.search);
            const orderId = urlParams.get('order');
            const receiptUrl = window.location.href;
            
            const totalElement = document.getElementById('total');
            const customerNameElement = document.getElementById('customer-name');
            const deliveryTypeElement = document.getElementById('delivery-type');
            const deliveryAddressElement = document.getElementById('delivery-address');
            
            const total = totalElement ? totalElement.textContent : '$0';
            const customerName = customerNameElement ? customerNameElement.textContent : 'Cliente';
            const deliveryType = deliveryTypeElement ? deliveryTypeElement.textContent : 'Retiro';
            const deliveryAddress = deliveryAddressElement ? deliveryAddressElement.textContent : null;
            
            const today = new Date();
            const currentDay = today.getDate();
            let paymentDate = new Date(today);
            
            // Si hoy es despu√©s del 21, el pago es el 21 del mes siguiente
            if (currentDay >= 21) {
                paymentDate.setMonth(paymentDate.getMonth() + 1);
            }
            paymentDate.setDate(21);
            
            const paymentMonth = paymentDate.toLocaleDateString('es-CL', { month: 'long' });
            
            let message = `üßæ *PEDIDO CONFIRMADO*\n`;
            message += `LA RUTA 11\n\n`;
            message += `üìã *Pedido:* ${orderId}\n`;
            message += `üë§ *Cliente:* ${customerName}\n`;
            message += `üöö *Entrega:* ${deliveryType}`;
            if (deliveryType === 'Delivery' && deliveryAddress) {
                message += ` - ${deliveryAddress}`;
            }
            message += `\n`;
            
            if (currentOrderData && currentOrderData.scheduled_time && currentOrderData.is_scheduled) {
                const scheduledDate = new Date(currentOrderData.scheduled_time);
                const options = { weekday: 'short', day: 'numeric', month: 'short', hour: '2-digit', minute: '2-digit' };
                const formattedDate = scheduledDate.toLocaleDateString('es-CL', options);
                message += `‚è∞ *Programado:* ${formattedDate}\n`;
            }
            
            // Agregar notas del cliente si existen
            if (currentOrderData && currentOrderData.customer_notes) {
                message += `üìù *Notas:* ${currentOrderData.customer_notes}\n`;
            }
            
            message += `üí∞ *Total:* ${total}\n\n`;
            
            // Agregar productos
            if (currentOrderData && currentOrderData.items && currentOrderData.items.length > 0) {
                message += `üõí *PRODUCTOS:*\n`;
                currentOrderData.items.forEach(item => {
                    message += `‚Ä¢ ${item.quantity}x ${item.name} - $${(item.price * item.quantity).toLocaleString('es-CL')}\n`;
                });
                message += `\n`;
            }
            
            message += `üéñÔ∏è *Pago con Cr√©dito RL6*\n`;
            message += `üìÖ *Pagas:* 21 de ${paymentMonth}\n\n`;
            message += `üîó Ver recibo completo:\n${receiptUrl}`;
            
            return encodeURIComponent(message);
        }

        function updateWhatsAppLink() {
            const message = generateWhatsAppMessage();
            const whatsappLink = document.getElementById('whatsapp-link');
            if (whatsappLink) {
                whatsappLink.setAttribute('href', `https://wa.me/56936227422?text=${message}`);
            }
        }

        const urlParams = new URLSearchParams(window.location.search);
        const orderId = urlParams.get('order');
        if (orderId) {
            const orderIdElement = document.getElementById('order-id');
            if (orderIdElement) {
                orderIdElement.textContent = orderId;
            }
            const receiptLink = document.getElementById('receipt-link');
            if (receiptLink) {
                receiptLink.textContent = window.location.href;
            }
        }

        async function loadOrderData() {
            if (!orderId) return;
            
            try {
                const response = await fetch(`/api/get_order.php?order_id=${orderId}`);
                const data = await response.json();
                
                if (data.success && data.order) {
                    currentOrderData = data.order;
                    displayOrderItems(
                        data.order.items, 
                        data.order.total, 
                        data.order.delivery_fee,
                        data.order.subtotal,
                        data.order.discount_amount || 0,
                        data.order.delivery_discount || 0,
                        data.order.delivery_extras || 0,
                        data.order.cashback_used || 0,
                        data.order.delivery_extras_items
                    );
                    displayCustomerInfo(data.order);
                    updateWhatsAppLink();
                }
            } catch (error) {
                console.error('Error:', error);
            }
        }

        function displayOrderItems(cart, total, deliveryFee, subtotalValue, discountAmount, deliveryDiscount, deliveryExtras, cashbackUsed, deliveryExtrasItems) {
            deliveryExtras = deliveryExtras || 0;
            cashbackUsed = cashbackUsed || 0;
            deliveryExtrasItems = deliveryExtrasItems || [];
            const orderItemsElement = document.getElementById('order-items');
            const subtotalElement = document.getElementById('subtotal');
            const totalElement = document.getElementById('total');
            const deliveryRowElement = document.getElementById('delivery-row');
            
            if (!orderItemsElement) return;
            
            let itemsHtml = '';
            let subtotal = 0;
            
            cart.forEach(item => {
                subtotal += item.price * item.quantity;
            });
            
            cart.forEach(item => {
                const itemTotal = item.price * item.quantity;
                
                const isCombo = item.type === 'combo' || item.category_name === 'Combos' || item.selections;
                let comboIncludesText = '';
                let customizationsText = '';
                
                if (isCombo) {
                    const comboIncludes = [];
                    
                    if (item.fixed_items && item.fixed_items.length > 0) {
                        item.fixed_items.forEach(f => {
                            if (typeof f === 'string') {
                                comboIncludes.push(`${item.quantity}x ${f}`);
                            } else {
                                const name = f.product_name || f.name || JSON.stringify(f);
                                comboIncludes.push(`${item.quantity}x ${name}`);
                            }
                        });
                    }
                    
                    if (item.selections && typeof item.selections === 'object') {
                        Object.values(item.selections).forEach(categoryItems => {
                            if (Array.isArray(categoryItems)) {
                                categoryItems.forEach(s => {
                                    const name = s.name || s.product_name || String(s);
                                    comboIncludes.push(`${item.quantity}x ${name}`);
                                });
                            } else if (categoryItems && typeof categoryItems === 'object') {
                                const name = categoryItems.name || categoryItems.product_name || String(categoryItems);
                                comboIncludes.push(`${item.quantity}x ${name}`);
                            }
                        });
                    }
                    
                    if (comboIncludes.length > 0) {
                        comboIncludesText = `<div class="item-details">‚Üí ${comboIncludes.join(', ')}</div>`;
                    }
                    
                    if (item.customizations && Array.isArray(item.customizations) && item.customizations.length > 0) {
                        const customItems = item.customizations.map(c => `${c.quantity || 1}x ${c.name || c.product_name} (+$${((c.price || 0) * (c.quantity || 1)).toLocaleString('es-CL')})`);
                        customizationsText = `<div class="item-details" style="color: #ff6b35; font-weight: 600;">+ ${customItems.join(', ')}</div>`;
                    }
                } else {
                    if (item.customizations && Array.isArray(item.customizations) && item.customizations.length > 0) {
                        const customItems = item.customizations.map(c => `${c.quantity || 1}x ${c.name || c.product_name} (+$${((c.price || 0) * (c.quantity || 1)).toLocaleString('es-CL')})`);
                        customizationsText = `<div class="item-details" style="color: #ff6b35; font-weight: 600;">${customItems.join(', ')}</div>`;
                    }
                }
                
                itemsHtml += `
                    <div class="item">
                        <div class="item-header">
                            <span>${item.quantity}x ${item.name}</span>
                            <span>$${itemTotal.toLocaleString('es-CL')}</span>
                        </div>
                        ${comboIncludesText}
                        ${customizationsText}
                    </div>
                `;
            });
            
            orderItemsElement.innerHTML = itemsHtml;
            
            if (subtotalElement) {
                const subtotalAfterDiscount = subtotal - discountAmount - cashbackUsed;
                subtotalElement.textContent = `$${subtotalAfterDiscount.toLocaleString('es-CL')}`;
            }
            
            if (totalElement) {
                totalElement.textContent = `$${total.toLocaleString('es-CL')}`;
            }
            
            if (deliveryFee > 0) {
                if (deliveryRowElement) {
                    deliveryRowElement.classList.remove('hidden');
                }
                const deliveryFeeElement = document.getElementById('delivery-fee');
                if (deliveryFeeElement) {
                    deliveryFeeElement.textContent = `$${deliveryFee.toLocaleString('es-CL')}`;
                }
            }
            
            if (deliveryExtras > 0 && deliveryExtrasItems) {
                const deliveryExtrasRow = document.getElementById('delivery-extras-row');
                if (deliveryExtrasRow) {
                    deliveryExtrasRow.classList.remove('hidden');
                }
                const extrasList = document.getElementById('delivery-extras-list');
                const extrasItems = typeof deliveryExtrasItems === 'string' ? JSON.parse(deliveryExtrasItems) : deliveryExtrasItems;
                if (extrasList && extrasItems) {
                    extrasList.innerHTML = extrasItems.map(extra => 
                        `<div style="display: flex; justify-content: space-between; margin: 3px 0;"><span>${extra.quantity}x ${extra.name}</span><span style="font-weight: bold;">$${(extra.price * extra.quantity).toLocaleString('es-CL')}</span></div>`
                    ).join('');
                }
                const extrasTotal = document.getElementById('delivery-extras-total');
                if (extrasTotal) {
                    extrasTotal.textContent = `$${deliveryExtras.toLocaleString('es-CL')}`;
                }
            }
            
            if (discountAmount > 0) {
                const productDiscountRow = document.getElementById('product-discount-row');
                if (productDiscountRow) {
                    productDiscountRow.classList.remove('hidden');
                }
                const productDiscountElement = document.getElementById('product-discount');
                if (productDiscountElement) {
                    productDiscountElement.textContent = `-$${discountAmount.toLocaleString('es-CL')}`;
                }
            }
            
            if (cashbackUsed > 0) {
                const cashbackRow = document.getElementById('cashback-row');
                if (cashbackRow) {
                    cashbackRow.classList.remove('hidden');
                }
                const cashbackElement = document.getElementById('cashback-amount');
                if (cashbackElement) {
                    cashbackElement.textContent = `-$${cashbackUsed.toLocaleString('es-CL')}`;
                }
            }
        }

        function displayCustomerInfo(order) {
            const customerInfoElement = document.getElementById('customer-info');
            const customerNameElement = document.getElementById('customer-name');
            const customerPhoneElement = document.getElementById('customer-phone');
            const deliveryTypeElement = document.getElementById('delivery-type');
            const deliveryAddressElement = document.getElementById('delivery-address');
            const deliveryAddressRowElement = document.getElementById('delivery-address-row');
            const customerNotesElement = document.getElementById('customer-notes');
            const customerNotesRowElement = document.getElementById('customer-notes-row');
            
            if (customerNameElement) {
                customerNameElement.textContent = order.customer_name || 'No especificado';
            }
            if (customerPhoneElement) {
                customerPhoneElement.textContent = order.customer_phone || 'No especificado';
            }
            if (deliveryTypeElement) {
                deliveryTypeElement.textContent = order.delivery_type === 'delivery' ? 'Delivery' : 'Retiro';
            }
            
            if (order.delivery_type === 'delivery' && order.delivery_address) {
                if (deliveryAddressElement) {
                    deliveryAddressElement.textContent = order.delivery_address;
                }
                if (deliveryAddressRowElement) {
                    deliveryAddressRowElement.classList.remove('hidden');
                }
            }
            
            if (order.customer_notes) {
                if (customerNotesElement) {
                    customerNotesElement.textContent = order.customer_notes;
                }
                if (customerNotesRowElement) {
                    customerNotesRowElement.classList.remove('hidden');
                }
            }
            
            if (customerInfoElement) {
                customerInfoElement.classList.remove('hidden');
            }
        }

        document.addEventListener('DOMContentLoaded', () => {
            // Calcular pr√≥xima fecha de pago (21 del mes actual o siguiente)
            const today = new Date();
            const currentDay = today.getDate();
            let paymentDate = new Date(today);
            
            if (currentDay >= 21) {
                paymentDate.setMonth(paymentDate.getMonth() + 1);
            }
            paymentDate.setDate(21);
            
            const paymentMonth = paymentDate.toLocaleDateString('es-CL', { month: 'long', year: 'numeric' });
            const paymentDateElement = document.getElementById('payment-date');
            if (paymentDateElement) {
                paymentDateElement.textContent = `21 de ${paymentMonth}`;
            }
            
            loadOrderData();
            if (typeof lucide !== 'undefined') {
                lucide.createIcons();
            }
        });
    </script>
</body>
</html>
