            letter-spacing: 1px;
        }
        .hidden { display: none; }
        @media (min-width: 768px) {
            body { padding: 20px; }
            .receipt { 
                border-radius: 16px; 
                box-shadow: 0 10px 40px rgba(0,0,0,0.1);
                min-height: auto;
            }
        }
        @media print {
            body { background: white; padding: 0; }
            .receipt { box-shadow: none; }
            .actions { display: none; }
        }
