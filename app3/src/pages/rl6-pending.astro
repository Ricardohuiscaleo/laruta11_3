        .btn-primary {
            background: #25d366;
            color: white;
            box-shadow: 0 4px 12px rgba(37, 211, 102, 0.3);
        }
        .btn-primary:active {
            transform: scale(0.98);
        }
        .btn-secondary {
            background: white;
            color: #666;
            border: 2px solid #e0e0e0;
        }
        .btn-secondary:active {
