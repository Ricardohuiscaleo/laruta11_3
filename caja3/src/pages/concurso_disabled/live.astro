        // Renderizar torneo
        function renderTournament(): void {
            const cuartosContainer = document.getElementById('cuartos-matches');
            const semifinalesContainer = document.getElementById('semifinales-matches');
            const finalContainer = document.getElementById('final-match');
            const championContainer = document.getElementById('champion-name');

            // Verificar que los contenedores existan
            if (!cuartosContainer || !semifinalesContainer || !finalContainer || !championContainer) {
                console.error('No se encontraron los contenedores necesarios');
                return;
            }

            // Limpiar contenedores
            cuartosContainer.innerHTML = '';
            semifinalesContainer.innerHTML = '';
            finalContainer.innerHTML = '';

            // Renderizar rondas
            tournamentData.rounds.forEach(round => {
                if (round.name === 'Cuartos de Final') {
                    cuartosContainer.innerHTML = round.matches.map(createMatch).join('');
                } else if (round.name === 'Semifinales') {
                    semifinalesContainer.innerHTML = round.matches.map(createMatch).join('');
                } else if (round.name === 'Final') {
                    finalContainer.innerHTML = round.matches.map(createMatch).join('');
                }
            });

            // Renderizar campe√≥n
            if (tournamentData.champion) {
                const champion = tournamentData.participants[tournamentData.champion];
                championContainer.textContent = champion ? champion.name.split(' ')[0] : 'CAMPE√ìN';
                const parentElement = championContainer.parentElement;
                if (parentElement) {
                    parentElement.classList.add('winner');
                }
            } else {
                championContainer.textContent = 'PENDIENTE';
                const parentElement = championContainer.parentElement;
                if (parentElement) {
                    parentElement.classList.remove('winner');
                }
            }

            // Actualizar timestamp
            const lastUpdateEl = document.getElementById('last-update');
            if (lastUpdateEl) {
                lastUpdateEl.textContent = `√öltima actualizaci√≥n: ${new Date().toLocaleTimeString()}`;
            }
        }

        // Fetch datos del torneo desde API
        async function fetchTournamentData(): Promise<void> {
            // Si las APIs no est√°n disponibles, usar datos demo directamente
            if (!isApiAvailable) {
                const demoData = DEMO_STATES[currentDemoIndex];
                currentDemoIndex = (currentDemoIndex + 1) % DEMO_STATES.length;
                tournamentData = demoData;
                renderTournament();
                return;
            }
            
            try {
                // Detectar entorno y usar ruta correcta
                const isDev = window.location.hostname === 'localhost' || window.location.hostname === '127.0.0.1';
                const apiUrl = isDev ? 'http://localhost/ruta11app/api/get_concurso_live.php' : '/api/get_concurso_live.php';
                
                // Obtener estado del torneo
                const tournamentResponse = await fetch(apiUrl + '?t=' + Date.now());
                
                if (!tournamentResponse.ok) {
                    throw new Error(`HTTP ${tournamentResponse.status}: ${tournamentResponse.statusText}`);
                }
                
                const adminData = await tournamentResponse.json();
                
                if (adminData && adminData.participants && adminData.matches) {
                    // Convertir formato admin a formato live
                    const participants: Record<string, Participant> = {};
                    adminData.participants.forEach((p: any) => {
                        participants[p.id] = { 
                            id: p.id, 
                            name: p.name, 
                            seed: p.seed, 
                            image_url: p.image_url 
                        };
                    });
                    
                    // Organizar matches por rondas
                    const rounds: Round[] = [];
                    const cuartos = adminData.matches.filter((m: any) => m.id.startsWith('Q'));
                    const semis = adminData.matches.filter((m: any) => m.id.startsWith('S'));
                    const final = adminData.matches.filter((m: any) => m.id.startsWith('F'));
                    
                    if (cuartos.length > 0) {
                        rounds.push({
                            name: 'Cuartos de Final',
                            matches: cuartos.map((m: any) => ({
                                id: m.id,
                                p1: m.p1.id,
                                p2: m.p2.id,
                                winner: m.winner ? m.winner.id : null,
                                status: m.status === 'completed' ? 'completed' : 'pending'
                            }))
                        });
                    }
                    
                    if (semis.length > 0) {
                        rounds.push({
                            name: 'Semifinales',
                            matches: semis.map((m: any) => ({
                                id: m.id,
                                p1: m.p1.id,
                                p2: m.p2.id,
                                winner: m.winner ? m.winner.id : null,
                                status: m.status === 'completed' ? 'completed' : 'pending'
                            }))
                        });
                    }
                    
                    if (final.length > 0) {
                        rounds.push({
                            name: 'Final',
                            matches: final.map((m: any) => ({
                                id: m.id,
                                p1: m.p1.id,
                                p2: m.p2.id,
                                winner: m.winner ? m.winner.id : null,
                                status: m.status === 'completed' ? 'completed' : 'pending'
                            }))
                        });
                    }
                    
                    // Encontrar campe√≥n
                    const champion = adminData.participants.find((p: any) => p.status === 'champion');
                    
                    tournamentData = {
                        participants,
                        rounds,
                        champion: champion ? champion.id : null,
                        status: adminData.status
                    };
                } else {
                    // Usar datos demo si no hay datos reales
                    const demoData = DEMO_STATES[currentDemoIndex];
                    currentDemoIndex = (currentDemoIndex + 1) % DEMO_STATES.length;
                    tournamentData = demoData;
                }
                
                renderTournament();
                
            } catch (error) {
                console.error('Error fetching tournament data:', error);
                
                // En desarrollo, usar datos demo si la API falla
                const isDev = window.location.hostname === 'localhost' || window.location.hostname === '127.0.0.1';
                if (isDev) {
                    console.log('üéÆ Usando datos demo para desarrollo');
                    const demoData = DEMO_STATES[currentDemoIndex];
                    currentDemoIndex = (currentDemoIndex + 1) % DEMO_STATES.length;
                    tournamentData = demoData;
                    renderTournament();
                    
                    const liveStatusEl = document.getElementById('live-status');
                    if (liveStatusEl) {
                        liveStatusEl.textContent = 'üéÆ MODO DEMO';
                        liveStatusEl.className = 'inline-block px-2 py-1 bg-blue-600 rounded-full text-xs font-bold animate-pulse';
                        liveStatusEl.style.fontSize = 'clamp(0.6rem, 1.5vw, 0.8rem)';
                    }
                } else {
                    const liveStatusEl = document.getElementById('live-status');
                    if (liveStatusEl) {
                        liveStatusEl.textContent = '‚ùå ERROR CONEXI√ìN';
                        liveStatusEl.className = 'inline-block px-2 py-1 bg-red-800 rounded-full text-xs font-bold';
