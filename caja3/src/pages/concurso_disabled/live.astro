            fetchTournamentData();
            
            if (isApiAvailable) {
                loadYouTubeVideo();
                loadChatMessages();
            } else {
                // Configurar placeholders para modo demo
                const embedContainer = document.getElementById('youtube-embed');
                if (embedContainer) {
                    embedContainer.innerHTML = '<span class="text-gray-500">Video no disponible en modo demo</span>';
                }
                
                const chatContainer = document.getElementById('chat-messages');
                if (chatContainer) {
                    chatContainer.innerHTML = '<div class="text-gray-500 text-sm text-center">Chat no disponible en modo demo</div>';
                }
            }
            
            // Eventos del chat (solo si APIs disponibles)
            if (isApiAvailable) {
                const sendButton = document.getElementById('send-chat');
                const chatInput = document.getElementById('chat-input') as HTMLInputElement;
                
                if (sendButton && chatInput) {
                    sendButton.addEventListener('click', () => {
                        sendChatMessage(chatInput.value);
                    });
                    
                    chatInput.addEventListener('keypress', (e) => {
                        if (e.key === 'Enter') {
                            sendChatMessage(chatInput.value);
                        }
                    });
                }
            }
            
            // Configurar polling
            setInterval(fetchTournamentData, isApiAvailable ? 1000 : 3000); // M√°s lento en demo
            
            if (isApiAvailable) {
                setInterval(loadYouTubeVideo, 60000); // Video cada 60 segundos
                setInterval(loadChatMessages, 3000); // Chat cada 3 segundos
                setInterval(trackViewer, 15000); // Track viewer cada 15 segundos
                setInterval(getViewersCount, 5000); // Actualizar conteo cada 5 segundos
                setInterval(loadParticipantLikes, 10000); // Actualizar likes cada 10 segundos
                
                // Track inicial
                trackViewer();
                loadParticipantLikes();
            } else {
                setInterval(getViewersCount, 3000); // Demo viewers cada 3 segundos
                getViewersCount(); // Inicial
                
                // Demo likes
                participantLikes = {'p1': 15, 'p2': 8, 'p3': 23, 'p4': 12, 'p5': 31, 'p6': 7, 'p7': 19, 'p8': 14};
            }
            
            // Iniciar countdown
            updateCountdown();
            setInterval(updateCountdown, 1000);
            
            console.log(`üèÜ Monitor de concurso iniciado - ${isApiAvailable ? 'Modo API' : 'Modo Demo'}`);
        });
    </script>
</body>
</html>