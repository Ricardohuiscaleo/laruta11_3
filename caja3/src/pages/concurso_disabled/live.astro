        // Crear elemento de participante
        function createParticipantBox(participantId: string, winnerId: string | null, isLive: boolean): string {
            const participant = tournamentData.participants[participantId];
            
            if (!participant) {
                return `<div class="participant-box bg-gray-800 border-2 border-gray-700 rounded-xl p-4 text-center text-gray-500 italic flex flex-col items-center">
                    <div class="w-24 h-30 bg-gray-700 rounded-lg mb-3 flex items-center justify-center">
                        <span class="text-gray-500 text-sm">Sin foto</span>
                    </div>
                    <div>[TBA]</div>
                </div>`;
            }

            const isWinner = winnerId === participantId;
            const isLoser = winnerId && winnerId !== participantId;
            
            let classes = 'participant-box border-2 rounded-xl p-2 text-center font-bold text-sm transition-all duration-300 flex flex-col items-center min-w-0 flex-1';
            
            if (isWinner) {
                classes += ' bg-green-600 border-green-400 text-white winner scale-105';
            } else if (isLoser) {
                classes += ' bg-gray-700 border-gray-600 text-gray-400 opacity-50';
            } else if (isLive) {
                classes += ' bg-blue-900 border-blue-400 text-white live-match';
            } else {
                classes += ' bg-gray-800 border-gray-600 text-gray-200';
            }

            const nameStyle = isLoser ? 'line-through' : '';
            const imageHtml = participant.image_url ? 
                `<img src="${participant.image_url}" class="w-24 h-30 object-cover rounded-lg mb-3" alt="${participant.name}" onerror="this.style.display='none'; this.nextElementSibling.style.display='flex';">` +
